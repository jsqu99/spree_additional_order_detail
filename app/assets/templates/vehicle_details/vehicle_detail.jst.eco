<form id="new_vehicle_detail">
  <label for="make">Make</label>
  <input type="text" name="make" id="new_vehicle_detail_make" value="<%= @vehicle_detail.get('make') %>">
  <label for="year">Year</label>
  <input type="text" name="year" id="new_vehicle_detail_year" value="<%= @vehicle_detail.get('year') %>">

  <br/>
  <input type="submit" value="Add">
</form>
